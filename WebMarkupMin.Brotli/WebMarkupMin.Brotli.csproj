<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.3</TargetFramework>
    <Description>Package that adds Brotli compression support to WebMarkupMin library. It uses BrotliSharpLib for compression.</Description>
    <Authors>Micha≈Ç Sykutera</Authors>
    <Version>1.0.3</Version>
    <PackageId>WebMarkupMin.Brotli</PackageId>
    <RepositoryUrl>https://github.com/msykutera/WebMarkupMin.Brotli</RepositoryUrl>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="BrotliSharpLib" Version="0.2.1" />
    <PackageReference Include="WebMarkupMin.AspNet.Common" Version="2.4.0" />
  </ItemGroup>
  
</Project>
